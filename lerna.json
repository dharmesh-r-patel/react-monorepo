{
  "version": "independent",
  "npmClient": "yarn",
  "packages": [
    "packages/*"
  ],
  "ignoreChanges": [
    "**/src/test/**",
    "**/src/*.test.*"
  ],
  "command": {
    "publish": {
      "conventionalCommits": true,
      "noGitTagVersion": true,
      "yes": true,
      "changelogPreset": "conventionalcommits"
    }
  }
}


